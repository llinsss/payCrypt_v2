{
    "name": "paycrypt-backend",
    "version": "1.0.0",
    "type": "module",
    "description": "TaggedPay Backend",
    "main": "server.js",
    "scripts": {
        "dev": "nodemon server.js",
        "start": "pm2 start ecosystem.config.cjs",
        "migrate": "knex migrate:latest && knex seed:run",
        "migrate:rollback": "knex migrate:rollback",
        "migrate:rollback:all": "knex migrate:rollback --all",
        "migrate:make": "knex migrate:make",
        "migrate:status": "knex migrate:status",
        "seed": "knex seed:run",
        "seed:make": "knex seed:make",
        "db:reset": "knex migrate:rollback --all && knex migrate:latest && knex seed:run"
    },
    "dependencies": {
        "autoswap-sdk": "^1.0.6",
        "axios": "^1.11.0",
        "bcrypt": "^5.1.0",
        "bignumber.js": "^9.3.1",
        "bull-board": "^1.7.2",
        "bullmq": "^5.61.0",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "dotenv": "^16.6.1",
        "ethers": "^6.15.0",
        "express": "^4.18.2",
        "express-rate-limit": "^6.8.1",
        "fs": "^0.0.1-security",
        "helmet": "^7.0.0",
        "joi": "^17.9.2",
        "jsonwebtoken": "^9.0.1",
        "knex": "^2.5.1",
        "mysql2": "^3.6.0",
        "p-limit": "^7.1.1",
        "pm2": "^6.0.13",
        "redis": "^5.8.2",
        "starknet": "^7.6.4"
    },
    "devDependencies": {
        "nodemon": "^3.0.1"
    }
}
