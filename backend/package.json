        "axios": "^1.11.0",
        "bcrypt": "^5.1.0",
        "bignumber.js": "^9.3.1",
        "body-parser": "^2.2.2",
        "bull-board": "^1.7.2",
        "bullmq": "^5.61.0",
        "cloudinary": "^2.8.0",
        "compression": "^1.8.1",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "dotenv": "^16.6.1",
        "ethers": "^6.15.0",
        "express": "^4.18.2",
        "express-basic-auth": "^1.2.1",
        "express-mongo-sanitize": "^2.2.0",
        "express-rate-limit": "^6.8.1",
        "fs": "^0.0.1-security",
        "graphql": "^16.12.0",
        "graphql-query-complexity": "^1.1.0",
        "helmet": "^7.2.0",
        "hpp": "^0.2.3",
        "ip-range-check": "^0.2.0",
