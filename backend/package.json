{
    "name": "paycrypt-backend",
    "version": "1.0.0",
    "type": "module",
    "description": "Tagged Backend",
    "main": "server.js",
    "scripts": {
        "dev": "nodemon server.js",
        "start": "node server.js",
        "migrate": "knex migrate:latest && knex seed:run",
        "migrate:rollback": "knex migrate:rollback",
        "migrate:rollback:all": "knex migrate:rollback --all",
        "migrate:make": "knex migrate:make",
        "migrate:status": "knex migrate:status",
        "seed": "knex seed:run",
        "seed:make": "knex seed:make",
        "db:reset": "knex migrate:rollback --all && knex migrate:latest && knex seed:run",
        "test:db": "node scripts/test-db-setup.js",
        "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --testPathPattern=tests/ --forceExit",
        "benchmark:balance": "node scripts/benchmark-balance-queries.js",
        "verify:indexes": "node scripts/verify-balance-indexes.js"
    },
    "jest": {
        "testEnvironment": "node",
        "transform": {}
    },
    "dependencies": {
        "@apollo/server": "^4.13.0",
        "@bull-board/api": "^6.13.0",
        "@bull-board/express": "^6.13.0",
